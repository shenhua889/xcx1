<!--pages/calendar/calendar.wxml-->
<view class='weeks box'>
  <block wx:for="{{['日','一','二','三','四','五','六']}}"  wx:key='index'>
    {{item}}
  </block>
</view>
<block wx:for="{{[1,2,3]}}"  wx:key='index'>
  <view>{{item}}</view>
</block>
<block wx:for="{{[1,2,3,4,5,6,7,8]}}"  wx:key='index'>
  {{item}}
</block>
